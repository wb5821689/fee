<?xml version="1.0" encoding="UTF-8"?>
<models
	xmlns="http://www.example.org/design-config"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.example.org/design-config ../../../data-dict.xsd ">
	<model
		modelName="com.landray.kmss.fs.base.model.FsBaseInvoicePool"
		messageKey="fs-base:table.fsBaseInvoicePool"
		displayProperty="fdInvoiceNo"
		serviceBean="fsBaseInvoicePoolService"
		table="fs_base_invoice_pool"
		url="/fs/base/fs_base_invoice_pool/fsBaseInvoicePool.do?method=view&amp;fdId=${fdId}">

		<idProperty>
			<generator
				type="assigned" />
		</idProperty>

		<simpleProperty
			name="fdInvoiceNo"
			messageKey="fs-base:fsBaseInvoicePool.fdInvoiceNo"
			type="String"
			column="fd_invoice_no"
			notNull="false"
			length="36"
			validate="false" />

		<simpleProperty
			name="fdInvoiceCode"
			messageKey="fs-base:fsBaseInvoicePool.fdInvoiceCode"
			type="String"
			column="fd_invoice_code"
			notNull="false"
			length="36"
			validate="false" />

		<simpleProperty
			name="fdCheckCode"
			messageKey="fs-base:fsBaseInvoicePool.fdCheckCode"
			type="String"
			column="fd_check_code"
			notNull="false"
			length="36"
			validate="false" />

		<simpleProperty
			name="fdInvoicePrice"
			messageKey="fs-base:fsBaseInvoicePool.fdInvoicePrice"
			type="String"
			column="fd_invoice_price"
			notNull="false"
			length="36"
			validate="false" />

		<simpleProperty
			name="fdInvoiceDate"
			messageKey="fs-base:fsBaseInvoicePool.fdInvoiceDate"
			type="String"
			column="fd_invoice_date"
			notNull="false"
			length="36"
			validate="false" />

		<simpleProperty
			name="docContent"
			messageKey="fs-base:fsBaseInvoicePool.docContent"
			type="RTF"
			column="doc_content"
			notNull="false"
			length="1000000"
			validate="false" />

		<simpleProperty
			name="fdModelId"
			messageKey="fs-base:fsBaseInvoicePool.fdModelId"
			type="String"
			column="fd_model_id"
			notNull="false"
			length="36"
			validate="false" />

		<simpleProperty
			name="fdModelName"
			messageKey="fs-base:fsBaseInvoicePool.fdModelName"
			type="String"
			column="fd_model_name"
			notNull="false"
			length="36"
			validate="false" />

		<simpleProperty
			name="fdDescription"
			messageKey="fs-base:fsBaseInvoicePool.fdDescription"
			type="String"
			column="fd_description"
			notNull="false"
			length="1500"
			validate="true" />

		<simpleProperty
			name="docStatus"
			messageKey="fs-base:fsBaseInvoicePool.docStatus"
			type="String"
			column="doc_status"
			notNull="true"
			length="2"
			validate="false" />

		<simpleProperty
			name="docCreateTime"
			messageKey="fs-base:fsBaseInvoicePool.docCreateTime"
			type="DateTime"
			column="doc_create_time"
			notNull="false"
			validate="false" />

		<simpleProperty
			name="fdLastModifiedTime"
			messageKey="fs-base:fsBaseInvoicePool.fdLastModifiedTime"
			type="DateTime"
			column="fd_last_modified_time"
			notNull="true"
			validate="false" />

		<modelProperty
			name="docCreator"
			messageKey="fs-base:fsBaseInvoicePool.docCreator"
			type="com.landray.kmss.sys.organization.model.SysOrgElement"
			column="doc_creator_id"
			notNull="false"
			validate="false" />

 

 
	</model>
</models>
