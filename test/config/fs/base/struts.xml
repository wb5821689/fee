<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">
<struts-config>
	<!-- 表单 Bean -->
	<form-beans>
		<form-bean
			name="fsBaseInvoicePoolForm"
			type="com.landray.kmss.fs.base.forms.FsBaseInvoicePoolForm" />
	</form-beans>

	<!-- 操作映射 -->
	<action-mappings>
		<action
			path="/fs/base/fs_base_invoice_pool/fsBaseInvoicePool"
			type="com.landray.kmss.fs.base.actions.FsBaseInvoicePoolAction"
			name="fsBaseInvoicePoolForm"
			parameter="method"
			input="edit"
			unknown="false"
			validate="true"
			scope="request">
			<forward
				name="view"
				path="/fs/base/fs_base_invoice_pool/fsBaseInvoicePool_view.jsp" />
			<forward
				name="view.4m"
				path="/fs/base/fs_base_invoice_pool/mobile/view.jsp" />
			<forward
				name="edit"
				path="/fs/base/fs_base_invoice_pool/fsBaseInvoicePool_edit.jsp" />
			<forward
				name="data"
				path="/fs/base/fs_base_invoice_pool/fsBaseInvoicePool_data.jsp" />
			<forward
				name="data.4m"
				path="/fs/base/fs_base_invoice_pool/mobile/list.jsp" />
		</action>


	</action-mappings>
	<!-- 消息资源 -->
	<message-resources
		parameter="com.landray.kmss.fs.base.ApplicationResources"
		key="fs-base" />
</struts-config>
