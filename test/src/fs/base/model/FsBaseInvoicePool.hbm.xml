<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.fs.base.model.FsBaseInvoicePool"
		table="fs_base_invoice_pool">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdInvoiceNo"
			column="fd_invoice_no"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdInvoiceCode"
			column="fd_invoice_code"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdCheckCode"
			column="fd_check_code"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdInvoicePrice"
			column="fd_invoice_price"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdInvoiceDate"
			column="fd_invoice_date"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="docContent"
			column="doc_content"
			update="true"
			insert="true"
			length="1000000"
			not-null="false"
			type="com.landray.kmss.common.dao.ClobStringType"
			lazy="true" />

		<property
			name="fdModelId"
			column="fd_model_id"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdModelName"
			column="fd_model_name"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdDescription"
			column="fd_description"
			update="true"
			insert="true"
			length="1500"
			not-null="false" />

		<property
			name="docStatus"
			column="doc_status"
			update="true"
			insert="true"
			length="2"
			not-null="true" />

		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdLastModifiedTime"
			column="fd_last_modified_time"
			update="true"
			insert="true"
			not-null="true" />

		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />


	</class>
</hibernate-mapping>
